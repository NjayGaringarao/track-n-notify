{
  "expo": {
    "name": "Track n' Notify",
    "slug": "track-n-notify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/track_n_notify-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/track_n_notify-adaptive-icon.png",
        "backgroundColor": "#87cefc"
      },
      "package": "com.prmsu.track_n_notify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/track_n_notify-splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#87cefc"
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Track n' Notify needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "Track n' Notify needs access to your Microphone."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0bccdfd0-42d9-4616-82ff-56a2fd1011fe"
      }
    },
    "owner": "zero-dev"
  }
}
